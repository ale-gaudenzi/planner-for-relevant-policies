(define (problem ZTRAVEL-1-2)
(:domain zeno-travel)
(:objects
	plane1
	person1
	person2
	city0
	city1
	city2
)
(:init
	(at plane1 city0)
	(aircraft plane1)
	(at person1 city0)
	(person person1)
	(at person2 city2)
	(person person2)
	(city city0)
	(city city1)
	(city city2)
)
(:goal (and
	(at plane1 city1)
	(at person1 city0)
	(at person2 city2)
	)
)

)
